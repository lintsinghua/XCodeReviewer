# Example Secrets Configuration
# Copy this file to secrets.yaml and replace with actual values
# DO NOT commit secrets.yaml to version control

apiVersion: v1
kind: Secret
metadata:
  name: xcodereviewer-secrets
  namespace: xcodereviewer
type: Opaque
stringData:
  database-url: "postgresql+asyncpg://user:password@postgres-host:5432/xcodereviewer"
  secret-key: "CHANGE-THIS-TO-SECURE-RANDOM-STRING"
  jwt-secret-key: "CHANGE-THIS-TO-DIFFERENT-SECURE-STRING"
  openai-api-key: "sk-your-openai-key"
  anthropic-api-key: "sk-ant-your-anthropic-key"
  github-token: "ghp_your-github-token"
  minio-access-key: "your-minio-access-key"
  minio-secret-key: "your-minio-secret-key"
